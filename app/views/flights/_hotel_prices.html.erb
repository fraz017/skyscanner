                                <div class="col-md-4">
                                  <span class="pull-right">
                                    <span class="price"><%= cookies["currency"] %> <%= agent["price_total"] %></span> &nbsp; 
                                    <% if agent["deeplink"] %>  
                                      <a href="<%= ENV["HOTEL_POLLING_URL"]+agent["deeplink"].to_s %>" target="_blank" class="btn btn-success">View Detail</a>
                                    <% end %>  
                                  </span>
                                </div>
                              </div>
                              <hr>
                            <% end %>
                          </p>
                        </div>
                        <div id="high_description_<%=hotel["hotel"]["hotel_id"]%>" class="tab-pane fade">
                          <h3>Description</h3>
                          <p>
                            <%= hotel["hotel"]["description"] %>
                          </p>
                        </div>
                        <div id="high_facilities_<%=hotel["hotel"]["hotel_id"]%>" class="tab-pane fade">
                          <h3>Facilities</h3>
                          <%hotel["amenities"].each do |am|%>
                            <div class="col-md-3">
                              <p>
                                <%= am["name"] %>
                                <%#= hotel["amenities"].map{ |s| s["name"]}.join(", ") %>
                              </p>  
                            </div>
                          <%end%>  
                        </div>
                        <div id="high_reviews_<%=hotel["hotel"]["hotel_id"]%>" class="tab-pane fade">
                          <h3>Reviews</h3>
                          <div class="row">
                            <h2 style="margin: 10px 0px 25px 15px;">Types of Guests</h2>
                            <div class="col-md-5 col-md-offset-1">
                              <% if hotel["guests"].present? %>
                                <% hotel["guests"].sort_by {|s| s["ratio"]}.each do |guest| %>
                                  <div class="row">
                                    <div class="rating">
                                      <%= guest["ratio"] %> %
                                    </div>
                                    <span class="rating-desc"><%= guest["description"] %></span>
                                  </div>
                                <% end %>
                              <% end %>
                            </div>
                            <div class="col-md-6">
                              <% if hotel["ratings"].present? %>
                                <% hotel["ratings"].each do |review| %>
                                  <div class="row">
                                    <div class="col-md-2">
                                      <div class="rating"><%= ((review["score"]/100.0)*10).round(1) %></div>
                                    </div>
                                    <div class="col-md-10">
                                      <div class="rating-desc">
                                        <%= review["name"] %>
                                      </div>
                                      <div class="review-examples">
                                        <p><%= review["examples"].join(", ") %></p>
                                      </div>
                                    </div>
                                  </div>  
                                <% end %>  
                              <% end %>  
                            </div>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div> 
                </div>
              </div>     
            </li>
          <% end %>
        </ul>
      <% end %>
      <div class="hotels-p text-center hidden"></div>
      <div class="hotels-pages text-center"></div>
    </div>
  </div>  
<% else %>
  <ul class="timeline timeline-inverse">
    <li>
      <div class="timeline-item">
        <h3 class="timeline-header">Sorry, No results found</h3>
        <div class="timeline-body">
          Please try again with different locations
        </div>
        <div class="timeline-footer">
          <p></p>
        </div>
      </div>
    </li>
  </ul>
<% end %>
<div id="myModal" class="modal fade" role="dialog">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4 class="modal-title">Out of date results</h4>
      </div>
      <div class="modal-body">
        <p>The availability of hotels may have changed during this time, would you like to refresh the prices?</p>
      </div>
      <div class="modal-footer">
        <div class="row">
          <div class="col-md-6">
            <a class="btn btn-primary" href="/" style="width: 100%">New search</a>
          </div>
          <div class="col-md-6">
            <a class="btn btn-primary refresh-cp" href="javascript:void(0)" style="width: 100%">Refresh</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script type="text/javascript">
  <% if params[:hotel].present? %>
    setTimeout(function(){ $("#myModal").modal(); }, 600000 );
  <% end %>
	var update = function(request){
    if (request == "PENDING" || request == "First"){
      $.ajax({
        type: 'GET',
        url: '/refresh_hotels.js',
        success: function () {
        }
      });
    }
  }

  update("<%= @status %>");  
  $(".hotels-details").hide();
  $("#most-popular").show();
  $('.hotels-details').Paginationwithhashchange2({    
    // default settings
    nextSelector: '.next',
    prevSelector: '.prev',
    counterSelector: '.counter',
    pagingSelector: '.hotels-p',
    itemsPerPage: 5,
    initialPage: 1
  });

  $(".hotels-pages").pagination({
    items: <%= @hotels.present? ? @hotels.count : 0 %>,
    itemsOnPage: 5,
    cssStyle: 'light-theme',
    hrefTextPrefix: '#',
    onPageClick: function(pageNumber, event){
      $(".hotels-p ."+pageNumber+" a").click();
    }
  });

  NProgress.done();

  $(document).ready(function($) {
    $(".fancy-images").click(function(){
      var get_id = $(this).parent().attr("class");
      $("a."+get_id).fancybox();
    });
    $(".refresh-cp").click(function(){
      $("#myModal").modal("hide");
      location.reload();
    });
    $(".sort-by").click(function(){
      $(".hotels-details").hide();
      var selected = $(this).val();
      $("#"+selected).show();
    });
  });

</script>