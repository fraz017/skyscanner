                                  </div>
                                  <div class="col-md-10">
                                    <div class="rating-desc">
                                      <%= review["name"] %>
                                    </div>
                                    <div class="review-examples">
                                      <p><%= review["examples"].join(", ") %></p>
                                    </div>
                                  </div>
                                </div>  
                              <% end %>  
                            <% end %>  
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div> 
              </div>
            </div>     
  	  		</li>
    		<% end %>
