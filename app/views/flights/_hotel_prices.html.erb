                                    <%= guest["ratio"] %> %
                                  </div>
                                  <span class="rating-desc"><%= guest["description"] %></span>
                                </div>
                              <% end %>
                            <% end %>
                          </div>
                          <div class="col-md-6">
                            <% if hotel["ratings"].present? %>
